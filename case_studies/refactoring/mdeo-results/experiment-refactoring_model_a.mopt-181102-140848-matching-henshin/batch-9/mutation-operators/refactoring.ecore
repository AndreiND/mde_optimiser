<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="refactoring" nsURI="http://momot.big.tuwien.ac.at/examples/refactoring/1.0"
    nsPrefix="refactoring">
  <eClassifiers xsi:type="ecore:EClass" name="Property" eSuperTypes="refactoring.ecore#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" eType="ecore:EClass refactoring.ecore#//Type"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entity" eSuperTypes="refactoring.ecore#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedAttribute" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Property"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="specialization" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Generalization" eOpposite="refactoring.ecore#//Generalization/general"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="generalization" eType="ecore:EClass refactoring.ecore#//Generalization"
        eOpposite="refactoring.ecore#//Generalization/specific"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Generalization">
    <eStructuralFeatures xsi:type="ecore:EReference" name="specific" lowerBound="1"
        eType="ecore:EClass refactoring.ecore#//Entity" eOpposite="refactoring.ecore#//Entity/generalization"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="general" lowerBound="1"
        eType="ecore:EClass refactoring.ecore#//Entity" eOpposite="refactoring.ecore#//Entity/specialization"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Type" eSuperTypes="refactoring.ecore#//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="RefactoringModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="properties" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Property" containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="entities" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Entity" containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="types" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Type" containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="generalizations" upperBound="-1"
        eType="ecore:EClass refactoring.ecore#//Generalization" containment="true"
        resolveProxies="false"/>
  </eClassifiers>
</ecore:EPackage>
